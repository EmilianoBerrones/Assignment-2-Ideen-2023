ense>\r\n  <v-dialog v-model=\"showDialog\" max-width=\"1000px\">\r\n    <v-card style=\"border-radius: 20px; padding: 30px\">\r\n      <v-card-title style=\"justify-content: center\">\r\n        <h1 style=\"font-size: xx-large\">Registro</h1>\r\n      </v-card-title>\r\n\r\n      <v-card-text class=\"texto-card\" style=\"line-height: 55px; font-size: large\">\r\n        El usuario ha sido creado exitosamente </v-card-text>\r\n\r\n      <v-card-actions>\r\n        <v-spacer></v-spacer>\r\n        <router-link to=\"/\">\r\n          <v-btn color=\"#384FFE\"\r\n                 style=\"font-size: large\"\r\n                 class=\"mr-9\"\r\n                 x-large\r\n                 text @click=\"showDialog=!showDialog\">INICIAR SESION\r\n          </v-btn>\r\n        </router-link>\r\n      </v-card-actions>\r\n    </v-card>\r\n  </v-dialog>\r\n\r\n  <v-row class=\"text-center\">\r\n    <v-col cols=\"12\">\r\n      <h1>CREA UNA CUENTA</h1>\r\n    </v-col>\r\n  </v-row>\r\n  <v-row justify=\"center\" style=\"margin-top: 0;\" dense>\r\n    <v-col cols=\"12\" md=\"5\">\r\n      <v-text-field\r\n        v-model=\"userName\"\r\n        label=\"Nombre completo\"\r\n        outlined\r\n        color=\"#384FFE\"\r\n      ></v-text-field>\r\n    </v-col>\r\n  </v-row>\r\n  <v-row justify=\"center\" style=\"margin-top: 0;\" dense>\r\n    <v-col cols=\"12\" md=\"5\">\r\n      <v-text-field\r\n        v-model=\"studentId\"\r\n        label=\"Matricula\"\r\n        outlined\r\n        color=\"#384FFE\"\r\n      ></v-text-field>\r\n    </v-col>\r\n  </v-row>\r\n  <v-row justify=\"center\" style=\"margin-top: 0;\" dense>\r\n    <v-col cols=\"12\" md=\"5\">\r\n      <v-text-field\r\n        v-model=\"institutionalEmail\"\r\n        label=\"Correo institucional\"\r\n        placeholder=\"Ejemplo: a01234567@tec.mx\"\r\n        :rules=\"emailRule\"\r\n        outlined\r\n        color=\"#384FFE\"\r\n      ></v-text-field>\r\n    </v-col>\r\n  </v-row>\r\n  <v-row justify=\"center\" 
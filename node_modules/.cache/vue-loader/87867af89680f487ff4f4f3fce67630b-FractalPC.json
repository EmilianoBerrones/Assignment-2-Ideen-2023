\n        outlined\r\n        color=\"#384FFE\"\r\n      ></v-text-field>\r\n    </v-col>\r\n  </v-row>\r\n  <v-row justify=\"center\" style=\"margin-top: 0;\" dense>\r\n    <v-col cols=\"12\" md=\"5\">\r\n      <v-text-field\r\n        v-model=\"emailInstitucional\"\r\n        label=\"Correo institucional\"\r\n        placeholder=\"Ejemplo: a01234567@tec.mx\"\r\n        :rules=\"emailRule\"\r\n        outlined\r\n        color=\"#384FFE\"\r\n      ></v-text-field>\r\n    </v-col>\r\n  </v-row>\r\n  <v-row justify=\"center\" style=\"margin-top: 0;\" dense>\r\n    <v-col cols=\"12\" md=\"5\">\r\n      <v-text-field\r\n        v-model=\"emailPersonal\"\r\n        label=\"Correo personal\"\r\n        placeholder=\"alguien@ejemplo.com\"\r\n        :rules=\"emailRule\"\r\n        outlined\r\n        color=\"#384FFE\"\r\n      ></v-text-field>\r\n    </v-col>\r\n  </v-row>\r\n  <v-row justify=\"center\" style=\"margin-top: 0;\" dense>\r\n    <v-col cols=\"12\" md=\"5\">\r\n      <v-text-field\r\n        v-model=\"password1\"\r\n        label=\"Contraseña\"\r\n        type=\"password\"\r\n        @input=\"checkPassword\"\r\n        outlined\r\n        color=\"#384FFE\"\r\n        :rules=\"passwordLength\"\r\n      ></v-text-field>\r\n    </v-col>\r\n  </v-row>\r\n  <v-row justify=\"center\" style=\"margin-top: 0;\" dense>\r\n    <v-col cols=\"12\" md=\"5\">\r\n      <v-text-field\r\n        v-model=\"password2\"\r\n        type=\"password\"\r\n        label=\"Confirmar contraseña\"\r\n        @input=\"checkPassword\"\r\n        outlined\r\n        color=\"#384FFE\"\r\n        :rules=\"passwordLength\"\r\n      ></v-text-field>\r\n    </v-col>\r\n  </v-row>\r\n  <v-row v-if=\"!passwordMatch\" justify=\"center\" style=\"margin-top: 0\" dense>\r\n    <v-col cols=\"12\" md=\"5\" style=\"color: red\">\r\n      Las contraseñas no coinciden\r\n    </v-col>\r\n  </v-row>\r\n  <v-row justify=\"center\" dense style=\"margin-top: 0\">\r\n    <v-col cols=\"12\" md=\"2\">\r\n      <v-checkbox label=\"Acepto terminos y condiciones\" color=\"#384FFE\"></v-checkbox>\r\n    </v-col>\r\n  </v-row>\r\n  <v-row justify=\"center\" style=\"margin-top: 50px\" dense>\r\n    <v-col cols=\"12\" md=\"5\">\r\n      <router-link to=\"/Index\">\r\n        <v-btn block color=\"#384FFE\" style=\"border-radius: 10px\" large>\r\n          <h2 style=\"color:white; font-size: medium\">Registrarse</h2>\r\n        </v-btn>\r\n      </router-link>\r\n    </v-col>\r\n  </v-row>\r\n</v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Register',\r\n  data () {\r\n    return {\r\n      password1: '',\r\n      password2: '',\r\n      passwordLength: [\r\n        value => value.length >= 6 || 'La contraseña debe tener al menos 6 caracteres'\r\n      ],\r\n      emailInstitucional: '',\r\n      emailPersonal: '',\r\n      emailRule: [\r\n        value => {\r\n          const pattern = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/\r\n          return pattern.test(value) || 'El correo electrónico no es válido'\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    checkPassword () {\r\n      if (this.password1 === this.password2) {\r\n        console.log('Las contrasenas coinciden')\r\n      } else {\r\n        console.log('Las contrasenas no coinciden')\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    passwordMatch () {\r\n      return this.password1 === this.password2\r\n    }\r\n  },\r\n  watch: {\r\n    password1 () {\r\n      this.checkPassword()\r\n    },\r\n    password2 () {\r\n      this.checkPassword()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]}]}